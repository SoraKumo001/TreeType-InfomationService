{
  "name": "tree-type-infomation-service",
  "version": "1.0.0",
  "description": "ツリー型情報掲載システム",
  "main": "dist/app/index.js",
  "scripts": {
    "build:front": "npx webpack -b",
    "watch:front": "npx webpack -b -w",
    "build:back": "npx tsc -b ",
    "watch:back": "npx tsc -b -w ",
    "build:jwf": "npx webpack -b --config local_modules/javascript-window-framework/src/webpack.config.js",
    "watch:jwf": "npx webpack -b -w --config local_modules/javascript-window-framework/src/webpack.config.js",
    "build:amf": "npx tsc -b local_modules/active-module-framework/src/",
    "watch:amf": "npx tsc -b -w local_modules/active-module-framework/src/",
    "build:all": "npm run build:amf && npm run build:back && npm run build:jwf && npm run build:front",
    "start": "node dist/app/index.js"
  },
