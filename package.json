{
  "name": "tree-type-infomation-service",
  "version": "1.0.0",
  "description": "ツリー型情報掲載システム",
  "main": "dist/app/index.js",
  "scripts": {
    "dev": "yarn dev:db && yarn watch",
    "dev:db": "docker-compose -p ttis-old -f docker/docker-compose.dev.yml up -d",
    "start": "cross-env NODE_ENV=production node --preserve-symlinks dist/app/index.js",
    "start:dev": "wait-on dist/app/index.js && node --preserve-symlinks dist/app/index.js",
    "watch": "npm-run-all -p watch:front watch:back start:dev",
    "build": "npm-run-all build:back build:front",
    "build:front": "webpack",
    "watch:front": "webpack -w",
    "build:back": "tsc -b ",
    "watch:back": "tsc -b -w ",
    "build:manager": "tsc -b local_modules/manager/src",
    "watch:jwf": "tsc -b -w local_modules/@jswf/core/src",
    "docker:start": "docker-compose -p ttis-old -f docker/docker-compose.yml up -d",
    "docker:build": "docker-compose -p ttis-old -f docker/docker-compose.yml build --parallel --compress --pull",
    "deploy": "yarn docker:build && docker save ttis-old_ttis | docker --tls -H croud.jp load && docker-compose --env-file .env-deploy -p ttis-old -f docker/docker-compose.yml up -d",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src"
  },
  "keywords": [],
  "author": "SoraKumo",
  "license": "MIT",
  "devDependencies": {
    "@jswf/core": "^0.1.10",
    "@types/browser-sync": "^2.26.1",
    "@types/capture-console": "~1.0.0",
    "@types/express": "~4.17.9",
    "@types/highlight.js": "~9.12.3",
    "@types/jsdom": "~16.2.5",
    "@types/pg": "~7.14.7",
    "@types/sprintf-js": "^1.1.2",
    "@types/sqlite3": "~3.1.6",
    "@types/uuid": "~8.3.0",
    "@types/webpack": "~4.41.25",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "~4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "analytics-gtag": "~1.0.2",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "~3.8.2",
    "css-loader": "~5.0.1",
    "eslint": "~7.17.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-plugin-prettier": "~3.3.1",
    "express": "~4.17.1",
    "highlight.js": "~10.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "mini-svg-data-uri": "^1.2.3",
    "node-sass": "~5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "sass-loader": "~10.1.0",
    "source-map-loader": "~2.0.0",
    "style-loader": "~2.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "~5.1.1",
    "ts-loader": "~8.0.14",
    "url-loader": "~4.1.1",
    "wait-on": "^5.2.1",
    "webpack": "~5.12.2",
    "webpack-cli": "~4.3.1"
  },
  "dependencies": {
    "@rfcs/adapter": "^1.0.0",
    "@rfcs/core": "^1.0.2",
    "browser-sync": "^2.26.13",
    "capture-console": "~1.0.1",
    "connect-browser-sync": "^2.1.0",
    "cross-env": "^7.0.3",
    "jsdom": "^16.5.0",
    "pg": "~8.5.1",
    "sprintf-js": "^1.1.2",
    "typeorm": "~0.2.29",
    "typescript": "^4.1.3",
    "xml2js": "~0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mofon001/TreeType-InfomationService.git"
  },
  "bugs": {
    "url": "https://github.com/mofon001/TreeType-InfomationService/issues"
  },
  "homepage": "https://github.com/mofon001/TreeType-InfomationService#readme"
}
