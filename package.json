{
  "name": "tree-type-infomation-service",
  "version": "1.0.0",
  "description": "ツリー型情報掲載システム",
  "main": "dist/app/index.js",
  "scripts": {
    "dev": "yarn dev:db && yarn watch",
    "dev:db": "docker-compose -p ttis-old -f docker/docker-compose.dev.yml up -d",
    "start": "cross-env NODE_ENV=production node --preserve-symlinks dist/app/index.js",
    "start:dev": "wait-on dist/app/index.js && nodemon --preserve-symlinks dist/app/index.js",
    "watch": "npm-run-all -p watch:front watch:back start:dev",
    "build": "npm-run-all build:front build:back",
    "build:front": "webpack",
    "watch:front": "webpack -w",
    "build:back": "tsc -b ",
    "watch:back": "tsc -b -w ",
    "watch:jwf": "tsc -b -w local_modules/@jswf/core/src",
    "docker:start": "docker compose -p ttis-old -f docker/docker-compose.yml up -d",
    "docker:build": "docker compose -p ttis-old -f docker/docker-compose.yml build --parallel --compress --pull",
    "docker:upload": "docker save  ttis-old-ttis | env-exec -e .env-deploy -- docker load",
    "deploy": "yarn docker:build && yarn docker:upload && env-exec -e .env-deploy -- docker compose -p ttis-old -f docker/docker-compose.yml up -d",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src"
  },
  "keywords": [],
  "author": "SoraKumo",
  "license": "MIT",
  "devDependencies": {
    "@jswf/core": "^0.1.10",
    "@types/browser-sync": "^2.26.3",
    "@types/capture-console": "~1.0.1",
    "@types/express": "~4.17.14",
    "@types/jsdom": "~20.0.1",
    "@types/pg": "~8.6.5",
    "@types/sprintf-js": "^1.1.2",
    "@types/sqlite3": "~3.1.8",
    "@types/uuid": "~8.3.4",
    "@types/webpack": "~5.28.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "~5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "analytics-gtag": "~1.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "~3.26.1",
    "css-loader": "~6.7.2",
    "env-exec": "^1.0.1",
    "eslint": "~8.28.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "express": "~4.18.2",
    "highlight.js": "~11.6.0",
    "mini-css-extract-plugin": "^2.7.0",
    "mini-svg-data-uri": "^1.4.4",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.7.1",
    "sass-loader": "~13.2.0",
    "source-map-loader": "~4.0.1",
    "style-loader": "~3.3.1",
    "svgo": "^3.0.2",
    "svgo-loader": "^3.0.3",
    "terser-webpack-plugin": "~5.3.6",
    "ts-loader": "~9.4.1",
    "url-loader": "~4.1.1",
    "wait-on": "^6.0.1",
    "webpack": "~5.76.0",
    "webpack-cli": "~5.0.0"
  },
  "dependencies": {
    "@rfcs/adapter": "^1.1.0",
    "@rfcs/core": "^1.1.3",
    "browser-sync": "^2.27.10",
    "capture-console": "~1.0.1",
    "connect-browser-sync": "^2.1.0",
    "cross-env": "^7.0.3",
    "jsdom": "^20.0.3",
    "pg": "~8.8.0",
    "sprintf-js": "^1.1.2",
    "typeorm": "~0.3.10",
    "typescript": "^4.9.3",
    "xml2js": "~0.4.23"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mofon001/TreeType-InfomationService.git"
  },
  "bugs": {
    "url": "https://github.com/mofon001/TreeType-InfomationService/issues"
  },
  "homepage": "https://github.com/mofon001/TreeType-InfomationService#readme"
}