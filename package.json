{
  "name": "tree-type-infomation-service",
  "version": "1.0.0",
  "description": "ツリー型情報掲載システム",
  "main": "dist/app/index.js",
  "scripts": {
    "watch:all": "npx concurrently \"npm:watch:front\" \"npm:watch:back\" \"npm:start\"",
    "build:all": "npm run build:amf && npm run build:back && npm run build:jwf && npm run build:manager && npm run build:front",
    "build:front": "npx webpack -b",
    "watch:front": "npx webpack -b -w",
    "build:back": "npx tsc -b ",
    "watch:back": "npx tsc -b -w ",
    "build:manager": "npx tsc -b local_modules/manager/src",
    "build:jwf": "npx tsc -b  local_modules/@jswf/core/src",
    "watch:jwf": "npx tsc -b -w local_modules/@jswf/core/src",
    "build:amf": "npx tsc -b local_modules/active-module-framework/src/",
    "watch:amf": "npx tsc -b -w local_modules/active-module-framework/src/",
    "start": "node --preserve-symlinks dist/app/index.js"
  },
  "keywords": [],
  "author": "SoraKumo",
  "license": "MIT",
  "devDependencies": {
    "@jswf/core": "file:local_modules/@jswf/core",
    "@jswf/manager": "file:local_modules/manager",
    "@types/capture-console": "~1.0.0",
    "@types/express": "~4.17.2",
    "@types/highlight.js": "~9.12.3",
    "@types/jsdom": "~12.2.4",
    "@types/pg": "~7.11.2",
    "@types/sprintf": "~0.1.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/sqlite3": "~3.1.5",
    "@types/uuid": "~3.4.6",
    "@types/webpack": "~4.41.0",
    "@types/xml2js": "~0.4.5",
    "@typescript-eslint/eslint-plugin": "~2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "analytics-gtag": "~1.0.2",
    "concurrently": "^5.0.0",
    "core-js": "~3.4.5",
    "css-loader": "~3.2.0",
    "dts-bundle": "~0.7.3",
    "dts-module-filter": "~1.0.0",
    "eslint": "~6.7.2",
    "eslint-config-prettier": "~6.7.0",
    "eslint-plugin-prettier": "~3.1.1",
    "express": "~4.17.1",
    "fs-extra": "~8.1.0",
    "hard-source-webpack-plugin": "~0.13.1",
    "highlight.js": "~9.16.2",
    "node-sass": "~4.13.0",
    "parse5": "~5.1.1",
    "prettier": "~1.19.1",
    "sass-loader": "~8.0.0",
    "source-map-loader": "~0.2.4",
    "sprintf": "~0.1.5",
    "style-loader": "~1.0.1",
    "terser-webpack-plugin": "~2.2.1",
    "ts-loader": "~6.2.1",
    "url-loader": "~3.0.0",
    "webpack": "~4.41.2",
    "webpack-cli": "~3.3.10"
  },
  "dependencies": {
    "@rfcs/adapter": "^1.0.0",
    "@rfcs/core": "^1.0.2",
    "@types/browser-sync": "^2.26.1",
    "browser-sync": "^2.26.7",
    "capture-console": "~1.0.1",
    "connect-browser-sync": "^2.1.0",
    "jsdom": "^15.2.1",
    "pg": "~7.14.0",
    "sprintf-js": "^1.1.2",
    "sqlite3": "^4.1.0",
    "typeorm": "~0.2.20",
    "typescript": "^3.7.2",
    "xml2js": "~0.4.22"
